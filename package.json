{
    "name": "easy-light",
    "displayName": "Easy Light",
    "description": "A VSCode theme for those who prefer light mode and avoid that halation effect",
    "version": "1.0.5",
    "publisher": "fransyrcc",
    "author": {
        "name": "Fransy"
    },
    "engines": {
        "vscode": "^1.53.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fransyrcc/easy-light-vscode-theme"
    },
    "bugs": {
        "url": "https://github.com/fransyrcc/easy-light-vscode-theme/issues"
    },
    "keywords": [
        "Theme",
        "Light Theme",
        "Easy Light",
        "Accessible Theme"
    ],
    "scripts": {
        "list": "npx vsce ls",
        "package": "npx vsce package",
        "publish": "npx vsce publish",
        "release": "bumpp package.json --commit --push --tag && npm -r publish --access public"
    },
    "galleryBanner": {
        "color": "#e0e0e0",
        "theme": "light"
    },
    "categories": [
        "Themes"
    ],
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Easy Light",
                "uiTheme": "vs",
                "path": "./themes/easy-light-color-theme.json"
            }
        ]
    },
    "devDependencies": {
        "bumpp": "^8.2.1"
    }
}
